"UnlitGeneric"
{
    "$basetexture" "vj_cofr/sprites/blood_red"
    "$vertexcolor"    1
    "$vertexalpha"    1
    "$nolod" 1
    "$translucent" 1
    //"$alphatest" 1
    "Proxies"
    {
        "AnimatedTexture"
        {
            "animatedtexturevar" "$basetexture"
            "animatedtextureframenumvar" "$frame"
