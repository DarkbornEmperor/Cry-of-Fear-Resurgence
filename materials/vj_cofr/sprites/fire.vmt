"UnlitGeneric"
{
    "$basetexture" "vj_cofr/sprites/fire"
    "$nolod" 1
    "$translucent" 1
    "$additive" "1"
    "$nocull" "1"
    "$vertexcolor" "1"
    "$overbrightfactor" "15"
    "Proxies"
    {
        "AnimatedTexture"
        {
            "animatedtexturevar" "$basetexture"
            "animatedtextureframenumvar" "$frame"
            "animatedtextureframerate" 10
        }
    }
}