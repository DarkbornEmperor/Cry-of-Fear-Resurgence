"UnlitGeneric"
{
    "$basetexture" "vj_cofr/sprites/soul_range"
    "$nolod" "1"
    "$translucent" "1"
    "$additive" "1"
    "$nocull" "1"
    "$vertexcolor" "1"
    "$overbrightfactor" "0.5"
    "Proxies"
    {
        "AnimatedTexture"
        {
            "animatedtexturevar" "$basetexture"
            "animatedtextureframenumvar" "$frame"
            "animatedtextureframerate" "15"
        }
    }
}
